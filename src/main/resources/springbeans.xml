<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">


<beans>
	<bean id="stud" class="com.entities.Student">
		<property name="stud_id" value="101"></property>
		<property name="studName" value="Payal"></property>
		<property name="city" value="Pune"></property>
	</bean>

	<bean class="com.entities.Employee">  <!-- using constructor-arg same output but pass index start from 0 -->
		<constructor-arg index="0" value="111"></constructor-arg>
		<constructor-arg index="1" value="Sarika"></constructor-arg>
		<constructor-arg index="2" value="87951213563"></constructor-arg>

	</bean>

</beans>